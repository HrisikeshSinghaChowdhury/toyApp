Users
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>UserName</th>
      <th>UserEmail</th>
      <th>UserCreated Time</th>
      <th>UserUpdated Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Sign Up', new_user_path %>
